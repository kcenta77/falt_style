                                        ];

                                    for (var i = 1; i <= 5; i++) {
                                        var child = document.getElementById('circles-' + i),
                                            percentage = 50 + (i * 10);

                                        Circles.create({
                                            id:         child.id,
                                            percentage: percentage,
                                            radius:     80,
                                            width:      15,
                                            number:   	percentage / 10,
                                            text:       '%',
                                            colors:     colors[i - 1]
                                        });
